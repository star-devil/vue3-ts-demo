{
  "name": "init-app",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "author": "wondersEGG",
  "scripts": {
    "serve": "pnpm dev",
    "dev": "NODE_OPTIONS=--max-old-space-size=4096 vite --mode dev",
    "pro": "NODE_OPTIONS=--max-old-space-size=4096 vite --mode pro",
    "test": "NODE_OPTIONS=--max-old-space-size=4096 vite --mode test",
    "build": "vue-tsc && vite build",
    "build:dev": "vue-tsc && vite build --mode dev",
    "build:pro": "vue-tsc && vite build --mode pro",
    "build:test": "vue-tsc && vite build --mode test",
    "preview": "vite preview",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src,build}/**/*.{vue,js,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{js,ts,json,tsx,css,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{html,vue,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint": "pnpm lint:eslint && pnpm lint:prettier && pnpm lint:stylelint",
    "commitlint": "commitlint --config .commitlint.config.cjs -e -V",
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@types/crypto-js": "^4.2.1",
    "ant-design-vue": "~4.0.7",
    "autoprefixer": "^10.4.17",
    "axios": "^1.6.1",
    "axios-abort": "^1.0.0",
    "axios-retry": "^4.0.0",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.10",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "postcss-pxtorem": "^6.0.0",
    "vue": "^3.3.4",
    "vue-router": "4",
    "vue-simple-password-meter": "^1.3.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@types/lodash": "^4.14.201",
    "@types/node": "^20.9.0",
    "@types/nprogress": "^0.2.3",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-vue": "^9.18.1",
    "husky": "^8.0.3",
    "lint-staged": ">=10",
    "postcss": "^8.4.31",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.0.3",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "stylelint": "^15.11.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-recommended-scss": "^13.1.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^11.1.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-scss": "^5.3.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "unplugin-auto-import": "^0.16.7",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^4.4.5",
    "vite-plugin-eslint": "^1.8.1",
    "vue-tsc": "^1.8.5"
  },
  "lint-staged": {
    "*.vue": [
      "eslint --fix",
      "stylelint --fix"
    ],
    "*.{js,jsx,ts,tsx}": "eslint --fix",
    "*.{htm,html,css,scss}": "stylelint --fix"
  },
  "browserslist": [
    "> 1%",
    "not ie 11",
    "not op_mini all",
    "not dead"
  ]
}